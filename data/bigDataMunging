  d3.csv("https://raw.githubusercontent.com/philrenaud/smu-lesson-2/master/SeriesPost.csv", function(data) {
    console.log('data acquired from .csv online:');
    console.log(data);
    bigData = data;
  }); //d3.csv



myCustomArray = [];

teams = _.uniq(_.pluck(bigData, "teamIDwinner"));

_.map(teams, function(team){
  var teamSeries = _.filter(bigData, function(obj){
    return obj.teamIDwinner == team || obj.teamIDloser == team;
  });

  var totalTeamWins = 0;
  var totalTeamLosses = 0;

  _.map(teamSeries, function(series){
    if (series.teamIDwinner == team){
      totalTeamWins = totalTeamWins + parseInt(series.wins);
    } else {
      totalTeamLosses = totalTeamLosses + parseInt(series.losses);
    }
  })

  console.log(team)
  console.log(teamSeries);
  console.log('team wins', totalTeamWins);
  console.log('team losses', totalTeamLosses);
  console.log('===================');

transformedObject = {
  "id": team,
  "stats": [
    {"name": "wins", "value": totalTeamWins},
    {"name": "losses", "value": totalTeamLosses},
  ]
}


myCustomArray.push(transformedObject);

})

JSON.stringify(myCustomArray);